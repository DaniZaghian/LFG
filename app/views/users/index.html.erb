 

<% @users.each do |user| %>

    <div class="container">
    	<div class="panel-heading">  </div>
		<div class="row panel-body">
		    <div class="row">
		    	<div class="col-md-3">Username: <%= user.username %> </div>
		    	<div class="col-md-3" > Full name: <%= user.first_name %> <%= user.last_name %> </div>
		    	<div class="col-md-3">Description: <%=  user.description %> </div>
		    </div>
		</div>
		<div class="panel-footer"></div>
    </div>

<% end %>
